#!/Users/45622/PycharmProjects/Bird_Simulation/venv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'buildozer==0.35','console_scripts','buildozer-remote'
__requires__ = 'buildozer==0.35'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('buildozer==0.35', 'console_scripts', 'buildozer-remote')()
    )
